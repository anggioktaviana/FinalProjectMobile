<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finalprojectmobile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:theme="@style/LoginTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddEditBukuActivity"
            android:parentActivityName=".DaftarBukuActivity"
            android:theme="@style/add" />
        <activity
            android:name=".AddEditPinjamActivity"
            android:parentActivityName=".DaftarPinjamActivity"
            android:theme="@style/add" />
        <activity
            android:name=".AddEditWishlistActivity"
            android:parentActivityName=".DaftarWishlistActivity"
            android:theme="@style/add" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".AboutActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".ProfileActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".DaftarBukuActivity" />
        <activity android:name=".DaftarPinjamActivity" />
        <activity android:name=".DaftarWishlistActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>